{
  "name": "@passageidentity/passage-js",
  "version": "3.12.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^16 || ^18"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc --noEmit & tsup ./src/index.ts --no-config  --sourcemap --format esm,cjs --dts --minify && npm run cdn:build",
    "build:watch": "tsc --noEmit & tsup ./src/index.ts --watch --no-config --format esm,cjs --dts --minify",
    "cdn:build": "tsup",
    "clean:dist": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "commit:pre": "npm run lint:staged",
    "config:build": "npm run log:env && env-cmd -f ./.env.$APP_ENV ./scripts/build-config.sh",
    "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
    "delete-map-files": "find ./dist ./cdn -name '*.map' -delete",
    "delete-references-to-map-files": "npm run delete-references-to-map-files:js && npm run delete-references-to-map-files:cjs",
    "delete-references-to-map-files:js": "find ./dist ./cdn -name '*.js' -exec sed -i -e 's/ sourceMappingURL=[^ ]*.map//g' {} +",
    "delete-references-to-map-files:cjs": "find ./dist ./cdn -name '*.cjs' -exec sed -i  -e 's/ sourceMappingURL=[^ ]*.map//g' {} +",
    "deploy:cloud-storage:dev": "gsutil -h 'Cache-Control:public, max-age=0' cp ./cdn/index.js gs://cdn.passage.dev/passage-js.js",
    "deploy:cloud-storage:uat": "gsutil -h 'Cache-Control:public, max-age=0' cp ./cdn/index.js gs://cdn-uat.passage.dev/passage-js.js",
    "deploy:cloud-storage:prod": "gsutil -h 'Cache-Control:public, max-age=0' cp ./cdn/index.js gs://cdn.passage.id/passage-js.js",
    "format": "pretty-quick --staged",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "eslint 'src/**/*.ts?(x)' --fix",
    "lint:staged": "npx lint-staged",
    "log:env": "echo Using MODE: $MODE && echo Using APP_ENV: $APP_ENV",
    "sentry:release": "env-cmd -f ./.env.$APP_ENV ./scripts/sentry-release.sh",
    "test": "vitest run --environment happy-dom",
    "test:watch": "vitest --environment happy-dom",
    "serve": "npx http-server ./cdn -p 4175 --cors -c-1"
  },
  "keywords": [],
  "author": "Passage Identity, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-config-passage": "*",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^12.4.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "@passageidentity/webauthnutils": "1.1.2",
    "@types/node": "^17.0.27",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "serialize-error": "^11.0.0"
  }
}
